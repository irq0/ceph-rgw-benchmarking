counter lines
counter rgw_ops_total by op, status, http_status
histogram rgw_op_request_time_seconds buckets 0.005, 0.01, 0.025, 0.05, 0.1, 0.25, 0.5, 1, 2.5, 5, 10 by op

/====== req done req=\S+ op=(?P<op>\S+).*status=(?P<status>\d+) http_status=(?P<http_status>\d+) latency=(?P<latency>\d+.\d+)s/ {
   rgw_ops_total[$op][$status][$http_status]++
   rgw_op_request_time_seconds[$op] = float($latency)
}

/$/ {
    lines++
}
